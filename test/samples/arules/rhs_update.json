{
    "rule-name": "Test_rhs_update",
    "salience": 100,
    "run-once": true,
    "when": [
        { "?course<-": "[Course.id == 1]"}

    ],
    "then": [
        {
           "update": {
                "model-id": "Course",
                "reflect-on-web": true,

                "data": {
                    "id": "?course.id",
                    "name": "Novo ime kursa.",
                    "start_date": "?course.start_date",
                    "end_date": "?course.end_date"
                }
            }
        }
    ]
}